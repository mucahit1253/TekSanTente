@model IQueryable<Product>

 
<main class="main">

    <!-- Page Title -->
    <div class="page-title" data-aos="fade">
      <div class="heading">
        <div class="container">
          <div class="row d-flex justify-content-center text-center">
            <div class="col-lg-8">
              <h1>Ürünler</h1>
            </div>
          </div>
        </div>
      </div>
      <nav class="breadcrumbs">
        <div class="container">
          <ol>
            <li><a asp-controller="Home" asp-action="index">Home</a></li>
            <li class="current">Ürünler</li>
          </ol>
        </div>
      </nav>
    </div><!-- End Page Title -->
     <section id="real-estate" class="real-estate section">
          <div class="container">
               <div class="row gy-4">
                @foreach (Product prd in Model)
                {
                   <partial name="_ProductCard" model="@prd"/>
                    
                }
                </div>
       </div>
    </section><!-- /Real Estate Section -->

    </main>